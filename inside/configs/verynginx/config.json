{
  "browser_verify_rule":[],
  "filter_rule":[{
      "enable":false,
      "action":"accept",
      "matcher":"localhost"
    },{
      "enable":true,
      "action":"block",
      "code":"403",
      "matcher":"attack_sql_0"
    },{
      "enable":true,
      "action":"block",
      "code":"403",
      "matcher":"attack_backup_0"
    },{
      "enable":true,
      "action":"block",
      "code":"403",
      "matcher":"attack_scan_0"
    },{
      "enable":true,
      "action":"block",
      "code":"403",
      "matcher":"attack_code_0"
    }],
  "readonly":false,
  "static_file_rule":[],
  "proxy_pass_rule":[],
  "summary_request_enable":true,
  "summary_group_persistent_enable":true,
  "base_uri":"/verynginx",
  "dashboard_host":"",
  "response":{
    "demo_response_html":{
      "content_type":"text/html",
      "body":"This is a html demo response"
    },
    "demo_response_json":{
      "content_type":"application/json",
      "body":"{\"msg\":\"soms text\",\"status\":\"success\"}"
    }
  },
  "frequency_limit_enable":true,
  "uri_rewrite_rule":[{
      "to_uri":"/verynginx/$1",
      "matcher":"demo_verynginx_short_uri",
      "replace_re":"^/vn/(.*)",
      "enable":true
    }],
  "cookie_prefix":"verynginx",
  "filter_enable":true,
  "browser_verify_enable":true,
  "scheme_lock_rule":[{
      "enable":false,
      "matcher":"verynginx",
      "scheme":"https"
    }],
  "frequency_limit_rule":[],
  "summary_with_host":false,
  "scheme_lock_enable":false,
  "matcher":{
    "attack_scan_0":{
      "UserAgent":{
        "value":"(nmap|w3af|netsparker|nikto|fimap|wget)",
        "operator":"≈"
      }
    },
    "all_request":{
    },
    "localhost":{
      "IP":{
        "value":"127.0.0.1",
        "operator":"="
      }
    },
    "demo_other_verynginx_uri":{
      "URI":{
        "value":"/redirect_to_verynginx",
        "operator":"="
      }
    },
    "attack_backup_0":{
      "URI":{
        "value":"\\.(htaccess|bash_history|ssh|sql)$",
        "operator":"≈"
      }
    },
    "attack_code_0":{
      "URI":{
        "value":"\\.(git|svn|\\.)",
        "operator":"≈"
      }
    },
    "attack_sql_0":{
      "Args":{
        "operator":"≈",
        "name_operator":"*",
        "value":"select.*from"
      }
    },
    "demo_verynginx_short_uri":{
      "URI":{
        "value":"^/vn",
        "operator":"≈"
      }
    },
    "verynginx":{
      "URI":{
        "value":"^/verynginx/",
        "operator":"≈"
      }
    }
  },
  "summary_temporary_period":60,
  "static_file_enable":true,
  "summary_group_temporary_enable":true,
  "backend_upstream":{
  },
  "redirect_enable":true,
  "uri_rewrite_enable":true,
  "admin":[{
      "enable":true,
      "password":"TESTPW",
      "user":"verynginx"
    }],
  "summary_collect_rule":[],
  "proxy_pass_enable":true,
  "config_version":"0.36",
  "redirect_rule":[{
